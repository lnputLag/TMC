<Window x:Class="TMC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TMC"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <GroupBox Header="Добавление картриджа" Margin="10">
            <StackPanel>
                <TextBox x:Name="CartridgeModelTextBox" Text="Модель картриджа" Margin="5"/>
                <TextBox x:Name="CartridgeQuantityTextBox" Text="Количество" Margin="5"/>
                <Button Content="Добавить картридж" Click="AddCartridgeButton_Click" Margin="5"/>
            </StackPanel>
        </GroupBox>
    </Grid>
</Window>
